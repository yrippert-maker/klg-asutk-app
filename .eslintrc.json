{
  "extends": ["next/core-web-vitals", "@typescript-eslint/recommended"],
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "no-eval": "error",
    "prefer-const": "error",
    "max-lines": ["warn", { "max": 500 }]
  },
  "ignorePatterns": ["node_modules/", ".next/", "out/", "scripts/"]
}
